{
  "parser": "babel-eslint",
  extends: "airbnb",
  ecmaFeatures: {
    jsx: true,
    modules: true
  },
  env: {
    browser: true,
    node: true
  },
  rules: {
    semi: [ 2, "never" ],
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  },
  plugins: [
    "babel",
    "react"
  ]
}
