{
  "presets": [
    'env',
    'flow',
  ],
  "plugins": [
    ['transform-react-jsx', { pragma: 'h' }],
    'transform-decorators-legacy',
    'syntax-trailing-function-commas',
    'transform-object-rest-spread',
    'transform-node-env-inline',
    'transform-react-remove-prop-types',
    'transform-class-properties',
  ],
  "env": {
    "production": {
      "presets": 'babili'
    }
  }
}
